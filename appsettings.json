    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./Logs/Log-.log",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "HydroPiContextConnection": "Server=(localdb)\\mssqllocaldb;Database=HydroPi;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}