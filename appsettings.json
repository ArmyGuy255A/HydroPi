{
  "HydroPiDatabaseSettings": {
    "Collections": {
      "Sensors": "Sensors",
      "SensorGroups": "SensorGroups",
      "Outputs": "Outputs",
      "OutputGroups": "OutputGroups",
      "DataPoints": "DataPoints",
      "Settings": "Settings",
      "Users": "Users",
      "Tasks": "Tasks",
      "Functions": "Functions"
    },
    "IdentityDatabaseName": "admin",
    "DatabaseName": "hydropi-data",
    "Host": "127.0.0.1:27017",
    "Username": "pi",
    "Password": "hydro"
  },
  "LiveReloadSettings": {
    "LiveReloadEnabled": false
  },
  "AllowedHosts": "*",
